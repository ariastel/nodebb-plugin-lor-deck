{
  "id": "nodebb-plugin-lor-deck",
  "library": "./library.js",
  "hooks": [
    {
      "hook": "static:app.load",
      "method": "init"
    },
    {
      "hook": "filter:composer.formatting",
      "method": "composerFormatting"
    },
    {
      "hook": "filter:parse.post",
      "method": "parsePost",
      "priority": 10
    },
    {
      "hook": "filter:parse.raw",
      "method": "parseRaw",
      "priority": 10
    }
  ],
  "staticDirs": {
    "static": "static",
    "decks": "decks"
  },
  "less": [
    "static/style.less"
  ],
  "scripts": [
    "static/main.js"
  ]
}